      <thingDefs>
        <li>Steel</li>
        <li>FSX</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Ammo_164mmCannonShell_Grape>1</Ammo_164mmCannonShell_Grape>
    </products>
  </RecipeDef>

  <!-- ==================实心 ================== -->
   <ThingDef Class="CombatExtended.AmmoDef" ParentName="164mmCannonShellBase">
